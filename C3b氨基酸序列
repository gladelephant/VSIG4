# 假设你已加载 full-length C3 序列
c3_full_seq <-""
# 提取 C3b 的保留片段
c3b_part1 <- substr(c3_full_seq, 1, 671)
c3b_part2 <- substr(c3_full_seq, 749, nchar(c3_full_seq))

# 拼接为 C3b 序列
c3b_seq <- paste0(c3b_part1, c3b_part2)
c3b_seq
# 输出长度和前几位预览
cat("C3b 长度:", nchar(c3b_seq), "\n")
cat("C3b 起始序列:", substr(c3b_seq, 1, 60), "...\n")
cat("C3b 长度:", nchar(c3_full_seq), "\n")
